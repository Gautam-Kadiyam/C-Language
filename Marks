#include<stdio.h>
#include<conio.h>
void main()
{
int n;
clrscr();
printf("Enter marks:");
scanf("%d",&n);
if((n>85)&&(n<100))
  printf("Grade A");
else if((n>70)&&(n<85))
  printf("Grade B");
else if((n>55)&&(n<70))
  printf("Grade C");
else if((n>40)&&(n<55))
  printf("Grade D");
else
  printf("Grade F");
getch();
}
